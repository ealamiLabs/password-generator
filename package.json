{
  "name": "@ealamilabs/password-generator",
  "version": "1.0.0",
  "description": "Dictionary based, easy-to-remember password generator.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp src/words.json dist/words.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "check:style": "npm run prettier:check && npm run lint",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ealamiLabs/password-generator.git"
  },
  "keywords": [
    "generator",
    "dictionary",
    "easy-to-remember",
    "password",
    "passwords"
  ],
  "author": "ealamiLabs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ealamiLabs/password-generator/issues"
  },
  "homepage": "https://github.com/ealamiLabs/password-generator#readme",
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@eslint/js": "^9.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.8",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "globals": "^15.1.0",
    "jest": "^29.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "pre-commit": [
    "lint",
    "prettier:check",
    "test:coverage"
  ]
}
